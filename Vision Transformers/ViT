### Epistemic Uncertainty

# shifts = int(1/dropout_rate)
y_pred_0 = model.predict(x_test, verbose = 0)
observed_prob = y_pred_0.max(axis = -1)
observed_cls = y_pred_0.argmax(axis = -1)
del y_pred_0

predictions = np.zeros((x_test.shape[0], shifts))
probablities = np.zeros((x_test.shape[0], num_classes, shifts))
for i, shift in enumerate(np.arange(1, shifts+1).tolist()):
    temp_model = modified_model(model, layer_name = "DROPOUT", rate = dropout_rate, new_layer = Dropout_Train, shift = shift)
    temp_pred = temp_model.predict(x_test, verbose = -1)
    predictions[:,i] = temp_pred.argmax(axis = -1)
    probablities[:,:,i] = temp_pred
    del temp_pred

def measure_dropout_uncertainty(observed_prob, observed_cls, predictions, probablities):
    # y_prob = model.predict(x_test)
    num = predictions.shape[0]
    axs = predictions.shape[-1]

    temp_pred = np.sort(predictions, axis = -1)
    predictions = predictions[temp_pred[:,-1].argsort()]
    probablities = probablities[temp_pred[:,-1].argsort()]

    observed_prob = observed_prob[temp_pred[:,-1].argsort()]
    observed_cls = observed_cls[temp_pred[:,-1].argsort()]

    mean_cls = predictions.mean(axis = -1)
    std_cls = predictions.std(axis = -1)

    unique_cls = np.unique(observed_cls)
    stds = []
    for i in unique_cls:
        arr = std_cls
        stds.append(std_cls[observed_cls == i].sum())

    mean_prob = probablities.mean(axis = -1)
    std_prob = probablities.std(axis = -1)

    # plt.figure(figsize = (24,12))
    fig, ((ax1, ax2), (ax3, ax4), (ax5, ax6)) = plt.subplots(nrows = 3, ncols = 2, sharex = False, sharey = False, figsize = (24, 15))
    x_range = np.arange(1, num+1, 1)

    ax1.plot(x_range, observed_cls, color = "blue", label = "observed_cls")
    ax1.set_xlabel("samples")
    ax1.set_ylabel("Class label")
    ax12 = ax1.twinx()
    ax12.set_ylabel("class_label+1")
    ax1.plot(x_range, mean_cls+1, color = "red", label = "mean_cls_label+1")
    ax1.legend()
    # ax12.legend()

    ax2.scatter(x_range, observed_prob, color = "blue", label = "observed_prob")
    ax2.set_xlabel("samples")
    ax2.set_ylabel("Class Probability")
    ax2.legend()

    # ax3.plot(x_range, observed_cls - mean_cls, color = "red", label = "observed cls-mean cls")
    ax3.plot(x_range, mean_cls + 2*std_cls, color = "cyan", label = "mean_cls + 2*std_cls")
    ax3.plot(x_range, mean_cls - 2*std_cls, color = "purple", label = "mean_cls - 2*std_cls")
    ax3.legend()

    ax4.scatter(x_range, mean_prob, color = "red", label = "mean_prob")
    ax4.set_xlabel("samples")
    ax4.set_ylabel("Class Probability")
    ax4.legend()

    # ax5.plot(x_range, observed_prob - mean_prob, color = "red", label = "observed prob-mean prob")
    ax5.plot(x_range, mean_prob + 2*std_prob, color = "cyan", label = "mean_prob + 2*std_prob")
    ax5.plot(x_range, mean_prob - 2*std_prob, color = "purple", label = "mean_prob - 2*std_prob")
    ax5.text(x_range[-1]*0.8, 0.8, "std in prob = " + str(std_prob.sum()), style='italic',bbox={'facecolor': 'red', 'alpha': 0.8, 'pad': 10})
    # ax5.plot([0],[0], color = "red", label = "std in prob = " + str(std_prob.sum()) )
    ax5.legend()

    ax6.bar(unique_cls, stds, label = "STD in cls prob")
    ax6.set_xlabel("classes")
    ax6.set_ylabel("Standard deviation")
    ax6.text(unique_cls[-1]*0.8, 0.0, "std in prob = " + str(np.sum(stds)), style='italic',bbox={'facecolor': 'red', 'alpha': 0.8, 'pad': 10})
    ax6.legend()

    plt.show()
    return

measure_dropout_uncertainty(observed_prob, observed_cls, predictions, probablities.max(axis = -2))

observed_cls = Class label predicted by core model

mean_cls = mean of class labels obtained by running model many times with different dropout layers

std_cls = Standard Deviation in Class labels obtained by running model many times with different dropout layers

observed_prob = Maximum probablities as obtained from core model say (sample_size, 1)

mean_prob = Mean of Maximum probabilites obtained by running model with different dropout

std_prob = standard deviation in maximum probabilites obtained by running model with different dropout

Details for Brier score, expected calibration error, static calibration error and reliability_diagram can be accessed from

https://lars76.github.io/2020/08/07/metrics-for-uncertainty-estimation.html

Brier score

$BS(p,\hat{p}) = \sum_{i=1}^c (\hat{p}_i - p_i)^2 = 1 - 2\hat{p}_j
 + \sum_{i=1}^c\hat{p}^2_i$



def brier_score(y_prob, y_test):
  return 1 + (np.sum(y_prob**2) - 2 * np.sum(y_prob[np.arange(y_prob.shape[0]), y_test])) / y_test.shape[0]
  # 1 + sum((y_prob)^2)  - 2(y_prob[:,y_test])

Expected Calibration Error

$ECE(p,\hat{p}) = \frac{1}{N}\sum_{b \in B}|\sum_{(\hat{p}_i, \hat{y}_i) \in b} 1(y_i = \hat{y}_i) - \hat{p}_i|$

def expected_calibration_error(y_prob, y_test, num_bins=15):
    pred_y = np.argmax(y_prob, axis=-1)
    correct = np.array(pred_y == y_test).astype(np.float32)
    prob_y = np.max(y_prob, axis=-1)

    b = np.linspace(start=0, stop=1.0, num=num_bins)
    bins = np.digitize(prob_y, bins=b, right=True)

    o = 0
    for b in range(num_bins):
        mask = bins == b
        if np.any(mask):
            o += np.abs(np.sum(correct[mask] - prob_y[mask]))

    return o / y_prob.shape[0]

Static Calibration Error

$SCB(B) = \frac{1}{NC}\sum_{c = 0}^{C-1}\sum_{b \in B}|\sum_{\hat{p}_i \in b} 1(y_i = c) - \hat{p}_i|$

def static_calibration_error(y_prob, y_test, num_bins=15):
  classes = y_prob.shape[-1]

  o = 0
  for cur_class in range(classes):
      correct = (cur_class == y_test).astype(np.float32)
      prob_y = y_prob[..., cur_class]

      b = np.linspace(start=0, stop=1.0, num=num_bins)
      bins = np.digitize(prob_y, bins=b, right=True)

      for b in range(num_bins):
        mask = bins == b
        if np.any(mask):
            o += np.abs(np.sum(correct[mask] - prob_y[mask]))

  return o / (y_prob.shape[0] * classes)

def reliability_diagram(probablities, y_test):
    rows = cols = int(np.ceil(np.sqrt(probablities.shape[-1])))
    # x, y = expected_calibration_error(probablities, y_test)
    fig, ax = plt.subplots(nrows = rows, ncols = cols, figsize = (18,10))
    for i in range(rows):
        for j in range(cols):
            s = i*cols + j
            if s <= probablities.shape[-1]:
                ax[i,j].plot()
                (ece, acc, conf, cnt, edges,bucket,) = tfp.stats.expected_calibration_error_quantiles(y_test, np.max(probablities[:,:,s], axis = -1), num_buckets=100)
                temp = min(max(acc), max(conf))
                # print(conf)
                ax[i,j].plot([0, temp], [0, temp], "k:", label="Perfectly calibrated")
                # ax[i,j].plot(conf, acc, "s-", label="model " + str(s))
                ax[i,j].scatter(conf, acc, label="model " + str(s+1))

                ax[i,j].set_xlabel("Confidence")
                ax[i,j].set_ylabel("Accuracy")
                ax[i,j].legend(loc="lower right")
                ax[i,j].set_title("Reliability diagram / calibration curve")

    plt.tight_layout()
    plt.show()

bs, ece, sce = [],[],[]
for i in range(shifts):
    bs.append(brier_score(probablities[:,:,i], y_test))
    ece.append(expected_calibration_error(probablities[:,:,i], y_test, num_bins=15))
    sce.append(static_calibration_error(probablities[:,:,i], y_test, num_bins=15))

print("Brier Score: ", np.mean(bs))
print("Expected Calibration Error: ", np.mean(ece))
print("Static Calibration Error: ", np.mean(sce))

