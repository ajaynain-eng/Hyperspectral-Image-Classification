# @keras_export("keras.layers.Attention")
class TransFormer(layers.Layer):
    def __init__(self, layer_num, num_heads, projection_dim, dropout, **kwargs):
        super(TransFormer, self).__init__(**kwargs)
        self.layer_num = layer_num
        self.num_heads = num_heads
        self.projection_dim = projection_dim
        self.dropout = dropout

    def build(self, input_shape):
        self.norm1 = LayerNormalization(epsilon=1e-6)
        self.norm2 = LayerNormalization(epsilon=1e-6)
        self.add1 = Weighted_add("transformer_1_" + str(self.layer_num))
        self.add2 = Weighted_add("transformer_2_" + str(self.layer_num))
        self.mha = MultiHeadAttention(num_heads = self.num_heads, key_dim = projection_dim, dropout = dropout)

        self.dense1 = Dense(self.projection_dim**2, activation=tf.keras.activations.gelu)
        self.dense2 = Dense(self.projection_dim, activation=tf.keras.activations.gelu)
        self.drop1 = Dropout(self.dropout)
        self.drop2 = Dropout(self.dropout)

    def call(self, input):
        x1 = self.norm1(input)
        x1 = self.mha(x1, x1)
        x1 = self.add1(x1, input)
        x2 = self.norm2(x1)
        x2 = self.drop2(self.dense2(self.drop1(self.dense1(x2))))
        return self.add2(x2, x1)

    def get_config(self, **kwargs):
        config = super(TransFormer, self).get_config()
        config.update({
            "layer_num": self.layer_num,
            "num_heads": self.num_heads,
            "projection_dim": self.projection_dim,
            "dropout": self.dropout
        })
        return config

# @keras_export("keras.layers.Attention")
class TransFormer_Block(layers.Layer):
    def __init__(self, num_layers, num_heads, projection_dim, dropout, **kwargs):
        super(TransFormer_Block, self).__init__(**kwargs)
        self.num_layers = num_layers
        self.num_heads = num_heads
        self.projection_dim = projection_dim
        self.dropout = dropout

    def build(self, input_shape):
        self.Blocks = [TransFormer(i, self.num_heads, self.projection_dim, self.dropout) for i in range(self.num_layers)]

    def call(self, input):
        block_list = []
        for i in range(self.num_layers):
            if i <= self.num_layers//2:
                input = self.Blocks[i](input)
                block_list.append(input)
            else:
                input = self.Blocks[i](input)
                input = Add()([input, block_list[self.num_layers - i - 1]])
        return input

    def get_config(self, **kwargs):
        config = super(TransFormer_Block, self).get_config()
        config.update({
            "num_layers": self.num_layers,
            "num_heads": self.num_heads,
            "projection_dim": self.projection_dim,
            "dropout": self.dropout,
            "Blocks": self.Blocks,
        })
        return config

def create_vit_classifier(input_shape = (P_S, P_S, B), num_classes = 7, use_pearson_corr = False, dropout_rate=0.25):
    inputs = layers.Input(shape=input_shape)                                                    # shape = (9,9,6)
    b = inputs.shape[0]
    # Create patches.
    if use_pearson_corr:
        x0 = Pearson_correlation_masked(P_S)(inputs)                                  # shape = (9,9,6)
    else:
        x0 = inputs

    patches = Patches(patch_size)(x0)                                                       # shape = 9, 3348 ----> 9 patches each shape 3*3*372 = 3348
    # Encode patches.
    encoded_patches = PatchEncoder(num_patches, projection_dim)(patches)                    # shape = 9, 2048
    # Create multiple layers of the Transformer block.
    encoded_patches = TransFormer_Block(transformer_layers, num_classes, projection_dim, dropout)(encoded_patches)

    encoded_patches = tf.identity(encoded_patches)
    encoded_patches = Dropout(dropout_rate, name = "TRAIN_DROPOUT_1")(encoded_patches)
    # Create a [batch_size, projection_dim] tensor.
    if method == " with_cls_tkn":
        representation = layers.LayerNormalization(epsilon=1e-6)(encoded_patches)[:,0,:]
    if method == " without_gap":
        representation = layers.LayerNormalization(epsilon=1e-6)(encoded_patches)
        representation = layers.Flatten()(representation)
    if method == " with_gap":
        representation = layers.LayerNormalization(epsilon=1e-6)(encoded_patches)
        representation = tf.expand_dims(representation, axis = -2)
        representation = GlobalAveragePooling2D()(representation)
        representation = layers.Flatten()(representation)

    # representation = Dropout(dropout_rate, name = "TRAIN_DROPOUT_2")(representation)
    # Add MLP.
    x = Dense(512, activation=tf.keras.activations.gelu)(representation)
    x = Dropout(dropout_rate, name = "TRAIN_DROPOUT_3")(x)
    x = Dense(256, activation=tf.keras.activations.gelu)(x)
    # x = Dropout(dropout_rate, name = "TRAIN_DROPOUT_4")(x)
    x = Dense(128, activation=tf.keras.activations.gelu)(x)
    x = Dropout(dropout_rate, name = "TRAIN_DROPOUT_5")(x)
    x = Dense(64, activation=tf.keras.activations.gelu)(x)
    features = Dropout(dropout_rate, name = "TRAIN_DROPOUT_6")(x)

    # Classify outputs.
    logits = layers.Dense(num_classes, activation='softmax')(features)
    # Create the Keras model.
    model = keras.Model(inputs=inputs, outputs=logits)
    return model

model = create_vit_classifier(input_shape = (P_S, P_S, B), num_classes = num_classes, use_pearson_corr = use_pearson_corr, dropout_rate = dropout_rate)
model.summary()

# plot model architecture
plot_model(model,to_file = folder_path + "model_architecture.png", show_shapes=False)

LR_START = 3e-4
LR_MAX = 6e-4
LR_MIN = 1e-7
LR_RAMPUP_EPOCHS = 0
LR_SUSTAIN_EPOCHS = 0
EPOCHS = epoch
STEPS = [epoch,epoch*2]

def lrfn(epoch):
    if epoch<STEPS[0]:
        epoch2 = epoch
        EPOCHS2 = STEPS[0]
    elif epoch<STEPS[0]+STEPS[1]:
        epoch2 = epoch-STEPS[0]
        EPOCHS2 = STEPS[1]
    elif epoch<STEPS[0]+STEPS[1]+STEPS[2]:
        epoch2 = epoch-STEPS[0]-STEPS[1]
        EPOCHS2 = STEPS[2]

    if epoch2 < LR_RAMPUP_EPOCHS:
        lr = (LR_MAX - LR_START) / LR_RAMPUP_EPOCHS * epoch2 + LR_START
    elif epoch2 < LR_RAMPUP_EPOCHS + LR_SUSTAIN_EPOCHS:
        lr = LR_MAX
    else:
        decay_total_epochs = EPOCHS2 - LR_RAMPUP_EPOCHS - LR_SUSTAIN_EPOCHS - 1
        decay_epoch_index = epoch2 - LR_RAMPUP_EPOCHS - LR_SUSTAIN_EPOCHS
        phase = math.pi * decay_epoch_index / decay_total_epochs
        cosine_decay = 0.5 * (1 + math.cos(phase))
        lr = (LR_MAX - LR_MIN) * cosine_decay + LR_MIN
    return lr

rng = [i for i in range(EPOCHS)]
lr_y = [lrfn(x) for x in rng]
plt.figure(figsize=(10, 4))
plt.plot(rng, lr_y, '-o')
print("Learning rate schedule: {:.3g} to {:.3g} to {:.3g}". \
          format(lr_y[0], max(lr_y), lr_y[-1]))
lr_callback = tf.keras.callbacks.LearningRateScheduler(lrfn, verbose = True)
plt.xlabel('Epoch',size=14)
plt.ylabel('Learning Rate',size=14)
plt.show()

